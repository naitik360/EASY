class Solution {
public:
    vector<int> twoSum(vector<int>& v, int t) {
        vector<int> res;
        unordered_map<int,int> mp;
        for(int i=0;i<v.size();i++){
            if(mp.count(t-v[i])){
                res.push_back(mp[t-v[i]]);
                res.push_back(i);
                return res;
            }
            else
                mp[v[i]]=i;
        }
        return  res;
    }
};
