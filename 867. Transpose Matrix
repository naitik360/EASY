class Solution {
public:
    vector<vector<int>> transpose(vector<vector<int>>& v) {
         vector<vector<int>> k;
        for(int i=0;i<v[0].size();i++)
        {   
            vector<int> t;
            for(int j=0;j<v.size();j++)
            {   
                t.push_back(v[j][i]);
            }
            k.push_back(t);
        }
      return k;
    }
};
