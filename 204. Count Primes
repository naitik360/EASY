class Solution {
public:
    
    int countPrimes(int n) {
        if(n<2)
             return 0;
        vector<bool> v(n,1);
        for(int i=2;i*i<n;i++)
        {
            if(v[i]==1)
            {
                for(int j=i*i;j<n;j+=i)
                    v[j]=0;
            }
        }
        int t=0;
        for(int i=2;i<n;i++)
        {
            if(v[i]==1)
                 t++;
        }
      return t;
    }
};
